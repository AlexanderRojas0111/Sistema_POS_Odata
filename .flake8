exclude = 
    .git,
    __pycache__,
    .venv,
    .venv_new,
    venv,
    env,
    .env,
    .pytest_cache,
    build,
    dist,
    *.egg-info,
    migrations,
    node_modules,
    frontend/build,
    frontend/node_modules
per-file-ignores =
    __init__.py:F401
    tests/*:F401,F403,F405
    app/__init__.py:F401 